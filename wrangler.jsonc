{
  // Cloudflare Pages デプロイ用（Git 連携時）
  // pages_build_output_dir により Pages が wrangler 設定を認識し、nodejs_compat が _worker.js バンドルに適用される
  // assets ブロックは含めない（ASSETS バインディング競合を回避、Pages が自動設定）
  "$schema": "./node_modules/wrangler/config-schema.json",
  "name": "pvsf",
  "pages_build_output_dir": ".open-next",
  "compatibility_date": "2025-02-01",
  "compatibility_flags": ["nodejs_compat", "global_fetch_strictly_public"]
}
